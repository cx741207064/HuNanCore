			$(".closes").click(function(){
				$(".qtcx_tax").hide();
			});

			var tken = $('#userToken').val();
			if(tken){
				initTzggUrl();
				$('#tzggModal').modal('show');
			}
		});

		var ctx = '/wsbs';

		function closeTzgg(){
			$('#tzggModal').modal('hide');
		}

		function post(url, params) {
			var temp = document.createElement("form");
			temp.action = url;
			temp.method = "post";
			temp.style.display = "none";
			for (var x in params) {
				var opt = document.createElement("input");
				opt.name = x;
				opt.value = params[x];
				temp.appendChild(opt);
			}
			document.body.appendChild(temp);
			temp.submit();
			return temp;
		}

		function get(url, params) {
			var temp = document.createElement("form");
			temp.action = url;
			temp.method = "get";
			temp.target = "_blank";
			temp.style.display = "none";
			for (var x in params) {
				var opt = document.createElement("input");
				opt.name = x;
				opt.value = params[x];
				temp.appendChild(opt);
			}
			document.body.appendChild(temp);
			temp.submit();
			return temp;
		}

		//sid单点登录出口退税预申报错误消息提示
		var sidCode = '';
		if(!isEmpty(sidCode)){
			var  sidLoginMsg= '';
			var sidUrl = '';

			if(sidCode=='000'){
				post(sidUrl);
			}
			else{
				layer.msg(sidLoginMsg ,{icon: 2});
			}
		}

		var  loginmessages= '';
		if(!isEmpty(loginmessages)){
			layer.msg(loginmessages ,{icon: 2});
		}

		//加载通知公告
		window.onload = loadJScript;
