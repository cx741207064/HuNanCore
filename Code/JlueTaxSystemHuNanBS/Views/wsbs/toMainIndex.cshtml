			ajaxRefreshOsCache();
			ajaxLoadNotice('index');
		}

		function forbiddenTips(obj){

			var name = $(obj).attr('id');
			var url = $(obj).attr('url');
			url = ctx + url;
			if(name == 'ds16' || name == 'ds17'){
				layer.confirm('所属期2019年1月起，经营所得个人所得税申报在自然人税收管理系统（ITS）办理。', {
					btn: ['确定']
				}, function () {
					var params = {};
					params.rsv_idx = name;
					get(url, params);
					layer.closeAll();
				});
			}else if(name == 'ds15'){
				layer.confirm('<a style="color:#2e8ded;cursor:pointer;" href="https://its.hntax.gov.cn" target="_blank">所属期2019及以后的个人所得税生产经营所得申报，请进入自然人办税服务平台网页WEB端办理！</a>', {
					btn: ['确定']
				}, function () {
					var params = {};
					params.rsv_idx = name;
					url = "https://its.hunan.chinatax.gov.cn"
					get(url, params);
					layer.closeAll();
				});

			}else if(name == 'FU081'){
				layer.alert("尊敬的纳税人，由于相关信息系统和软件升级的原因，企业所得税汇算清缴年度纳税申报功能模块将于2019年3月11日正式上线，由此给您带来的不便我们深表歉意！");
			}else{
				layer.confirm('电子税务局不支持纳税人进行属期2019年的个人所得税申报，相关业务请进入自然人税收管理系统（ITS）办理。', {
					btn: ['确定']
				}, function () {
					var params = {};
					params.rsv_idx = name;
					get(url, params);
					layer.closeAll();
				});
			}

		}



		$(function () {
			jQuery.blockUI.defaults.message = '<img src="/wsbs/images/loading/loading.gif"/>';
			var bindgs = $('#bindgs').val();
			var nsrztDm = $('#nsrztDm').val();
			var loginBz = $('#loginBz').val();
			var roleId = $('#roleId').val();


			// 用户退出事件
            $('#logout_btn').on('click', function (e) {
                return false;
				unionIndex.logout('/wsbs');
			});

			$('#login_btn').on('click', function (e) {
				unionIndex.login('/wsbs');
			});

			//申报提醒
			var sbtx = store.get("sbtx_data");
			if(sbtx && sbtx.result && sbtx.result.wsbsz){
				alert(sbtx.result.wsbtx);
				store.remove("sbtx_data");
			}
		});

		function initTzggUrl(){
			var menuList=store.get('pc_menuList');
			var nsrztDm = $('#nsrztDm').val();
			if(!isEmpty(menuList)){
				$("#tzggModal").find("a").each(function(i,e) {
					if(menuList=='nobody' || !arrayContainsItem(menuList, $(this).attr("attrId"))){
						var clazz = $(this).attr("class") || '';
						if($(this).attr("attrId") != '' && $(this).attr("attrId") != undefined){
							$(this).removeAttr('href');
							$(this).removeAttr("onclick");
							var imageSrc = $(this).find("img").attr("src");
							var imagesName = imageSrc.substring(imageSrc.lastIndexOf('/'), imageSrc.length);
							$(this).find("img").attr("src", ctx + "/images/icon_img_hui/" + imagesName);
							$(this).click(function(){
								if(nsrztDm == '04'){
									layerMsg("用户状态为停业，不能进行此操作！",7);
								}else{
									layerMsg("您暂时没有权限使用该模块。",7);
								}
							});
						}
					}else{
						for(var i = 0;i < menuList.length; i++) {
							if(menuList[i].yyfwDm==$(this).attr("attrId")){
								$(this).attr("href",ctx+menuList[i].url);
								$(this).addClass('c4585eb');
								$(this).unbind();
							}
						}
					}
				});
			}
		}


	</script>
